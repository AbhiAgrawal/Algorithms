class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        dictionary = dict()
        # 1 pass
        for i in range(len(nums)):
            complement = target - nums[i]
            if complement in dictionary.keys():
                return [dictionary.get(complement), i]
            else:
                dictionary[nums[i]] = i
        return None        
