class Solution:


####not using split https://leetcode.com/problems/reverse-words-in-a-string/solutions/2808468/python3-faster-than-97-without-using-split/
    def reverseWords(self, s: str) -> str:
#         _RE_COMBINE_WHITESPACE = re.compile(r"\s+")
#         s = _RE_COMBINE_WHITESPACE.sub(" ", s).strip()
#         s = s[::-1] ##reverse full string
#         l,r = 0,0
#         for r in range(len(s)):
#             if s[r] == ' ': ##space is encountered
#                 s[l:r+1] = s[r:l-1:-1]
#                 l=r+1
#                 start,end = l
                
#         return s 
          return " ".join(s.split()[::-1]) 
                
            
        
        
        
        
