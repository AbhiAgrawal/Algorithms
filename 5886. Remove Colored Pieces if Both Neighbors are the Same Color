class Solution:
    def winnerOfGame(self, colors: str) -> bool:
        n = len(colors)
        remove = "A"
        start, curr = 0, 1
        A_totalmoves, B_totalMoves = 0,0
        while curr<n:
            while curr<n and colors[start] == colors[curr]:
                curr+=1
            diff = curr-start
            if diff>=3:
                if colors[start] == "A":
                    A_totalmoves+=(diff-2)
                else:
                    B_totalMoves+=(diff-2)
            start = curr
        print(A_totalmoves, "    ",B_totalMoves)    
        if  A_totalmoves >  B_totalMoves:
            return True
        else:
            return False
                


            
                
            
            
        
